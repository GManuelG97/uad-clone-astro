{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components": [
        "src/components/*"
      ],
      "@/assets": [
        "src/assets/*"
      ],
      "@/lib": [
        "src/lib/*"
      ],
      "@/layout": [
        "src/layout/*"
      ],},
    "jsx": "react-jsx",
    "jsxImportSource": "react",
  }
  
}